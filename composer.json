{
  "name": "stephenlake/laravel-sanity",
  "description": "Automate builds and code standards sniffing. Now with badges.",
  "keywords": ["automate", "ci", "testing", "badges"],
  "license": "MIT",
  "type": "project",
  "authors": [{
    "name": "Stephen Lake",
    "email": "stephen@closurecode.com"
  }],
  "require": {
    "phpunit/phpunit": "~7.0",
    "squizlabs/php_codesniffer": "^3.4",
    "kitetail/zttp": "^0.4.0"
  },
  "require-dev": {
    "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
    "orchestra/testbench": "~3.4.0|~3.5.0|~3.6.0"
  },
  "autoload": {
    "psr-4": {
      "Sanity\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sanity\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sanity\\SanityEventServiceProvider",
        "Sanity\\SanityServiceProvider"
      ]
    }
  }
}
